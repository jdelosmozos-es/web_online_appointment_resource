<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">
        <record id="menu_appointment" model="website.menu">
            <field name="name">Appointment</field>
            <field name="url">/appointment</field>
            <field name="parent_id" ref="website.main_menu"/>
            <field name="sequence" type="int">1</field>
        </record>
        <record id="weekoff_day0" model="web.online.appointment.weekoff">
            <field name="name">Monday</field>
            <field name="dayofweek">0</field>
        </record>
        <record id="weekoff_day1" model="web.online.appointment.weekoff">
            <field name="name">Tuesday</field>
            <field name="dayofweek">1</field>
        </record>
        <record id="weekoff_day2" model="web.online.appointment.weekoff">
            <field name="name">Wednesday</field>
            <field name="dayofweek">2</field>
        </record>
        <record id="weekoff_day3" model="web.online.appointment.weekoff">
            <field name="name">Thursday</field>
            <field name="dayofweek">3</field>
        </record>
        <record id="weekoff_day4" model="web.online.appointment.weekoff">
            <field name="name">Friday</field>
            <field name="dayofweek">4</field>
        </record>
        <record id="weekoff_day5" model="web.online.appointment.weekoff">
            <field name="name">Saturday</field>
            <field name="dayofweek">5</field>
        </record>
        <record id="weekoff_day6" model="web.online.appointment.weekoff">
            <field name="name">Sunday</field>
            <field name="dayofweek">6</field>
        </record>
        <record id="appointment_system_user" model="res.users">
        	<field name="name">Appointment System User</field>
        	<field name="login">Appointment System User</field>
        </record>
    </data>
</odoo>
